{
  "name": "@flashport/flashportui",
  "version": "0.0.10",
  "description": "This is a set basic UI components for FlashPort",
  "author": "Kenny Lerma",
  "license": "MIT",
  "homepage": "https://flashport.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlashPort-org/FlashPortUI.git"
  },
  "bugs": {
    "url": "https://github.com/FlashPort-com/FlashPortUI/issues"
  },
  "keywords": [
    "FlashPortUI",
    "UI",
    "FlashPort",
    "Flash",
    "Components",
    "Canvas",
    "Canvas2d"
  ],
  "type": "module",
  "main": "./dist/flashportui.umd.cjs",
  "module": "./dist/flashportui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/flashportui.js",
      "require": "./dist/flashportui.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "release": "npm publish --access public"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.0.0-beta.1",
    "vite-tsconfig-paths": "^3.5.1"
  },
  "dependencies": {
    "@flashport/flashport": "^0.0.10"
  }
}
